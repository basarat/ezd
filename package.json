{
  "name": "eze",
  "version": "0.0.0",
  "description": "Ez demos / docs for Component / Styleguide authors",
  "main": "lib/index.js",
  "scripts": {
    "buildtsc": "tsc -p .",
    "builddocs": "ts-node ./src/docs/main.tsx",
    "test": "npm run buildtsc && npm run builddocs",
    "start": "concurrently -k \"npm run buildtsc -- -w\" \"nodemon --watch src --ext ts,tsx --exec npm run builddocs\"",
    "publishdocs": "ts-node ./scripts/publishdocs.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "1.0.0",
    "highlight.js": "9.9.0",
    "marked": "0.3.6",
    "ts-loader": "1.2.0",
    "ts-node": "1.5.2",
    "typescript": "2.2.0-dev.20161130",
    "webpack": "1.14.0"
  },
  "devDependencies": {
    "@types/escape-html": "0.0.19",
    "@types/fs-extra": "0.0.35",
    "@types/highlight.js": "9.1.9",
    "@types/react": "0.14.43",
    "@types/react-dom": "0.14.18",
    "@types/webpack": "2.1.0",
    "concurrently": "3.1.0",
    "csstips": "0.0.3",
    "escape-html": "1.0.3",
    "gh-pages": "0.11.0",
    "nodemon": "1.11.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "typestyle": "0.19.0"
  }
}
